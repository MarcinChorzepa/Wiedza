+/**
+ * Created by Marcin on 21.04.2017.
+ */
+public class OperatorySkrocone {
+    public static void main(String argsp[]){
+        int x=10;
+        x+=5;
+        System.out.println(x);
+        x*=3;
+        System.out.println(x);
+        x/=5;
+        System.out.println(x);
+        x-=2;
+        System.out.println(x);
+        x%=4;
+        System.out.println(x);
+
+
+    }
+
+}
